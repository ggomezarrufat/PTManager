{
  "functions": {
    "api/auth/login.js": {
      "maxDuration": 10
    },
    "api/auth/refresh.js": {
      "maxDuration": 10
    },
    "api/auth/register.js": {
      "maxDuration": 10
    },
    "api/auth/logout.js": {
      "maxDuration": 10
    },
    "api/auth/me.js": {
      "maxDuration": 10
    },
    "api/clock/state.js": {
      "maxDuration": 10
    },
    "api/clock/join.js": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}